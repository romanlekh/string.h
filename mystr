#include "myString.h"

static const char endSymb = '~';
//end-string symbol (analog to standard '\0')

void* lmemcpy(void * destination, const void * source, size_t num)
{
    char* dest = (char*)destination;
    char* src = (char*)source;
    size_t i = 0;
    while(i<num)
    {
        *(dest + i) = *(src + i);
        ++i;
    }
    return dest;
}


